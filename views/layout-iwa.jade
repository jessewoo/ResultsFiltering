block all
doctype html
html
  head
    block head
      title= title
      // css
      block css
      link(rel='stylesheet', href='/css/bootstrap-3.3.6.min.css')
      link(rel='stylesheet', href='/css/tuning-iwa.css')
    block meta
      meta(name="theme-color" content="#ffffff")
      meta(charset='utf-8')
      meta(http-equiv='X-UA-Compatible', content='IE=edge')
      meta(name='viewport', content='width=device-width, initial-scale=1')
      meta(name='description', content='Coding Test for Intuit')
      meta(name='author', content='Jesse Woo for Intuit interview')

  body
    .navbar.navbar-default.navbar-fixed-top.navbar(role='navigation')
        .container
            button.pull-right.btn.btn-default.btn-sm#StartForm Create New
            button.pull-right.btn.btn-primary.btn-sm.hide#SubmitForm Submit
            .navbar-header
                button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
                    span.sr-only Toggle navigation
                    span.icon-bar
                    span.icon-bar
                    span.icon-bar
                a.navbar-brand(href='/iwa') IWA.com

        block nav

    .container
        block content

        block footer
          footer
            // Common javascript includes
            script(src='/3rd-party-js/jquery-2.1.4.min.js')
            script(src='/3rd-party-js/bootstrap-3.3.6.min.js')
            script(src='/3rd-party-js/jquery.dataTables-1.10.10.min.js')
            script(src='/3rd-party-js/dataTables.bootstrap-1.10.10.min.js')
            // Other javascript files
            block js-includes
